<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox spacing="5.0" styleClass="toolbar-color" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cs211.project.controllers.components.NavbarAdminController">
   <children>
      <Label text="Admin Panel" textFill="#eeeeee">
         <font>
            <Font size="20.0" />
         </font>
         <VBox.margin>
            <Insets left="20.0" top="20.0" />
         </VBox.margin>
      </Label>
      <Button mnemonicParsing="false" onAction="#onUserLogClick" prefHeight="24.0" style="-fx-background-color: TRANSPARENT;" text="User Log" textFill="#eeeeee">
         <padding>
            <Insets top="-1.0" />
         </padding>
         <font>
            <Font size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <graphic>
            <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../images/icon-about-us.png" />
               </image>
            </ImageView>
         </graphic>
         <VBox.margin>
            <Insets left="30.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
